<div class="row">
  <div class="col-8 offset-2">
      <%= simple_form_for user_contact do |f| %>
        <%= f.input :first_name, label: "Please enter your first name:" %>
        <%= f.input :last_name, label: "Please enter your last name:" %>
        <%= f.submit "OK", class: "btn btn-primary" %>
      <% end %>
  </div>
</div>
